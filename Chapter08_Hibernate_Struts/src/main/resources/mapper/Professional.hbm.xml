<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.fcsca.model.Professional" table="professional" schema="sh">
        <id name="id" column="id"/>
        <property name="professionalName" column="professional_name"/>
        <property name="studnetNumber" column="studnet_number"/>
        <property name="counselor" column="counselor"/>
    </class>
</hibernate-mapping>